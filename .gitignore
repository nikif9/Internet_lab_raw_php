# Игнорирование директорий зависимостей
/vendor/              # Composer зависимости
/node_modules/        # Node.js зависимости (если используются)

# Игнорирование временных файлов и логов
*.log                # Все файлы логов
logs/                # Директория логов
tmp/                 # Временные файлы
cache/               # Кэш проекта
*.cache              # Файлы кэша
*.sqlite             # SQLite базы данных
# Игнорирование файлов окружения
.env                 # Файл окружения
.env.*              # Файлы окружения с суффиксом

# Игнорирование настроек IDE и редакторов
.idea/              # PhpStorm/WebStorm
.vscode/            # VS Code

# Игнорирование системных файлов
.DS_Store           # macOS
Thumbs.db           # Windows

# Игнорирование артефактов сборки
/build/             # Сборочные артефакты
